{
	"name": "Vanita",
	"description": "Enhances the Roblox website experience with extra functionality.",
	"version": "2.2.0",
	"manifest_version": 3,
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"matches": ["https://www.roblox.com/*"],
			"js": ["embedded.js"],
			"run_at": "document_end"
		}
	],
	"host_permissions":[
		"https://www.roblox.com/*"
	],
	"icons": {
		"128": "logo.png"
	},
	"web_accessible_resources": [
		{
			"resources": [
				"logo.png"
			],
			"matches": [
				"https://www.roblox.com/*"
			]
		}
	]
}