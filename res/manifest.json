{
	"$schema": "https://json.schemastore.org/chrome-manifest",

	"key": "",

	"manifest_version": 3,

	"name": "Vanita",
	"description": "Save, and wear Roblox outfits with nice-to-have features like importing, exporting, and tagging.",
	"version": "3.3.0",

	"permissions": [
		"activeTab",
		"storage",
		"unlimitedStorage",
		"scripting",
		"identity"
	],

	"optional_permissions": [
		"downloads"
	],

	"host_permissions": [
		"https://www.googleapis.com/*"
	],

	"icons": {
		"128": "logo.png"
	},

	"action": {
		"default_popup": "index.html"
	},

	"background": {
		"service_worker": "THIS INFORMATION WILL BE FULFILLED BY TRANSFORM SCRIPT",
		"type": "module"
	},

	"oauth2": {
		"client_id": "1092575241546-f2bfa23o76eqd3apmtk5bllj94pa6uuf.apps.googleusercontent.com",
		"scopes": [
			"https://www.googleapis.com/auth/drive.appdata",
			"https://www.googleapis.com/auth/drive.file"
		]
	}
}